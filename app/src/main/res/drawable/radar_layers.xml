<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#FFFFFF"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <item android:id="@+id/topo">
        <bitmap
            android:src="@drawable/lwx_topo_short"
            android:gravity="center"
            />
    </item>
    <item android:id="@+id/county">
        <bitmap
            android:src="@drawable/lwx_county_short"
            android:gravity="center"
            />
    </item>
    <item android:id="@+id/highways">
        <bitmap
            android:src="@drawable/lwx_highways_short"
            android:gravity="center"
            />
    </item>
    <item android:id="@+id/city">
        <bitmap
            android:src="@drawable/lwx_city_short"
            android:gravity="center"
            />
    </item>
    <item android:id="@+id/rivers">
        <bitmap
            android:src="@drawable/lwx_rivers_short"
            android:gravity="center"
            />
    </item>
    <item android:id="@+id/radar">
        <bitmap
            android:src="@drawable/lwx_20180725_1924_layer_listn0r"
            android:gravity="center"
            />
    </item>
</layer-list>
